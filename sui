import com.eviware.soapui.support.XmlHolder
import groovy.json.JsonSlurper
import org.openqa.selenium.By
import org.openqa.selenium.WebDriver
import org.openqa.selenium.WebElement
import org.openqa.selenium.support.ui.Select;
import org.openqa.selenium.Keys

import org.openqa.selenium.chrome.ChromeDriver
import org.openqa.selenium.support.ui.ExpectedCondition
import org.openqa.selenium.support.ui.WebDriverWait
import java.net.*

import com.eviware.soapui.SoapUI
import com.eviware.soapui.settings.ProxySettings

log.info "PROXY OFF!"
SoapUI.settings.setString( ProxySettings.ENABLE_PROXY, "false")
SoapUI.saveSettings();
SoapUI.updateProxyFromSettings(); 
//SoapUI.updateProxyFromSettings();



def loginUrl = context.expand( '${#TestSuite#loginUrl}' )
def client_id = context.expand( '${#TestSuite#client_id}' )
def scope = context.expand( '${Properties#scope}' )
def countryCode = context.expand( '${Properties#countryCode}' ).toUpperCase()
def businessCode = context.expand( '${Properties#businessCode}' ).toUpperCase()
def redirectUri = context.expand( '${#TestSuite#redirect_uri}' )
def state = context.expand( '${Properties#state}' )
def authzurl = loginUrl + "&client_id="+client_id + "&scope="+scope+"&countryCode=" + countryCode +"&businessCode="+businessCode+"&locale=en_US"+"&state="+state+"&redirect_uri="+redirectUri
log.info("authzurl is: " + authzurl)


System.setProperty("webdriver.chrome.driver", context.expand( '${#Project#chromedriver}' ));
WebDriver driver = new ChromeDriver();

// Go to Login Page

driver.get(authzurl)



WebElement form = driver.findElement(By.name("LoginForm"))

//- text input  user/pwd
WebElement uName = driver.findElement(By.name("j_username_mask"))
WebElement uPass = driver.findElement(By.name("j_password"))


uName.sendKeys(context.expand( '${Properties#userName}' ))
uPass.sendKeys(context.expand( '${Properties#password}' ))

// Authenticate User
WebElement loginButton = driver.findElement(By.id("submit_login"))
loginButton.click()

//Give Consent
WebElement consentPageAuthBtn = driver.findElement(By.id("submit_authorize"))
consentPageAuthBtn.click()

//Extract Code from URL

def qStr=new URI(driver.getCurrentUrl()).query

log.info("url is: " + qStr)


def qStrArr=qStr.split('&')

def authCode=qStrArr[0].split('=')[1]

driver.close()


log.info("auth code is: " + authCode)

def code = authCode.split(/&/)[0]
if (code)
{
	context.setProperty("code",code)
}


log.info "PROXY OFF!"
SoapUI.settings.setString( ProxySettings.ENABLE_PROXY, "true")
SoapUI.saveSettings();
SoapUI.updateProxyFromSettings(); 

